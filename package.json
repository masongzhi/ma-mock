{
  "name": "mock",
  "version": "0.1.0",
  "description": "用于mock和proxy的服务器",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"nodemon -w ./server/app/ ./server/index.js\" \"cd ./client && npm run dev\" ",
    "start": "node ./server/index.js",
    "install": "cd ./client && npm install",
    "format": "prettier --write '{client/src,server/app}/**/*.{js,vue,scss,css}'",
    "build": "cd ./client && npm run build",
    "precommit": "concurrently \"npm run format\" \"cd ./client && npm run lint\""
  },
  "keywords": [
    "mock",
    "proxy",
    "koa"
  ],
  "author": "511071161@qq.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:masongzhi/mock-proxy.git"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "debug": "^3.1.0",
    "joi": "^13.4.0",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-spa-static": "^1.0.0",
    "lodash": "^4.17.10",
    "strip-json-comments": "^2.0.1",
    "tracer": "^0.9.0"
  },
  "devDependencies": {
    "concurrently": "^3.6.0",
    "husky": "^0.14.3",
    "prettier": "^1.13.7"
  }
}
